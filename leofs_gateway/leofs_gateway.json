{
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "constraints": [
    [
      "hostname",
      "UNIQUE"
    ]
  ],
  "container": {
    "docker": {
      "forcePullImage": true,
      "image": "devopsbq/leofs_gateway:1.2.8.consul",
      "network": "HOST",
      "privileged": true
    },
    "type": "DOCKER"
  },
  "cpus": 0.5,
  "env": {
    "CONF_CONSUL": "consul.999urls.com:8500"
  },
  "executor": "",
  "healthChecks": [
  ],
  "id": "/leofs/gateway",
  "instances": 1,
  "maxLaunchDelaySeconds": 300,
  "mem": 128,
  "ports": [
  ],
  "requirePorts": true,
  "tasksRunning": 1,
  "tasksStaged": 0,
  "upgradeStrategy": {
    "maximumOverCapacity": 0.2,
    "minimumHealthCapacity": 0.5
  }
}
